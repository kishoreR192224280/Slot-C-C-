#include <iostream>

int main() {
    
    int integerNum;
    float floatNum;

    std::cout << "Enter an integer: ";
    std::cin >> integerNum;

    std::cout << "Enter a float: ";
    std::cin >> floatNum;

    float result = floatNum / integerNum;

    std::cout << "Result of division (float / integer): " << result << std::endl;

    return 0;
}
