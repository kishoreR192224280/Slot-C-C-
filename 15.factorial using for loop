#include <iostream>

using namespace std;

int main() {
    int num;
    unsigned long long factorial = 1; 
    cout << "Enter a positive integer: ";
    cin >> num;
    if (num < 0) {
        cout << "Error: Factorial is not defined for negative numbers.";
    }
    else {
       
        for (int i = 1; i <= num; ++i) {
            factorial *= i;
        }

        cout << "Factorial of " << num << " = " << factorial;
    }

    return 0;
}
