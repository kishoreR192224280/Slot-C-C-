#include <iostream>

int main() {
    const int maxSize = 100; // Change this according to your needs
    int arr[maxSize];
    int size, sum = 0;

    std::cout << "Enter the size of the array: ";
    std::cin >> size;

    std::cout << "Enter " << size << " elements:\n";
    for (int i = 0; i < size; ++i) {
        std::cin >> arr[i];
    }

    for (int i = 0; i < size; ++i) {
        sum += arr[i];
    }

    std::cout << "Sum of array elements: " << sum << std::endl;

    return 0;
}
