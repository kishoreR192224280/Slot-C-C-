#include <iostream>

inline int square(int num) {
    return num * num;
}

int main() {
    int number;

    std::cout << "Enter a number: ";
    std::cin >> number;

    int squaredNumber = square(number);

    std::cout << "The square of " << number << " is: " << squaredNumber << std::endl;

    return 0;
}
